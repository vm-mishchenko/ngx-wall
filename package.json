{
    "name": "ngx-wall",
    "version": "1.1.14",
    "repository": {
        "url": "https://github.com/vm-mishchenko/ngx-wall.git",
        "type": "git"
    },
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build ngx-wall",
        "copy:scss": "cpx \"projects/ngx-wall/src/lib/resources/**/*\" \"dist/ngx-wall\"",
        "release": "npm run test:once && ng build ngx-wall --prod && npm run copy:scss",
        "gh": "ng build --stats-json --prod --base-href /ngx-wall/",
        "test": "ng test ngx-wall",
        "test:once": "ng test ngx-wall --watch=false --code-coverage",
        "test:cli": "ng test ngx-wall --watch=false  --code-coverage --no-progress --browsers=ChromeNoSandbox",
        "lint": "ng lint ngx-wall",
        "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
        "e2e": "ng e2e"
    },
    "private": true,
    "dependencies": {
        "@angular/common": "^6.0.0",
        "@angular/core": "^6.0.0",
        "@angular/cdk": "^6.0.0",
        "@angular/compiler": "^6.0.0",
        "@angular/animations": "^6.0.0",
        "@angular/forms": "^6.0.0",
        "@angular/http": "^6.0.0",
        "@angular/platform-browser": "^6.0.0",
        "@angular/platform-browser-dynamic": "^6.0.0",
        "@angular/router": "^6.0.0",
        "core-js": "^2.5.4",
        "rxjs": "^6.0.0",
        "zone.js": "^0.8.26",
        "codemirror": "5.37.0",
        "@ng-bootstrap/ng-bootstrap": "2.0.0",
        "open-iconic": "1.1.1",
        "bootstrap": "4.0.0",
        "ngx-sticky-modal": "^0.0.3"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.6.1",
        "@angular-devkit/build-ng-packagr": "~0.6.3",
        "@angular/cli": "~6.0.1",
        "@angular/compiler-cli": "^6.0.0",
        "@angular/language-service": "^6.0.0",
        "@types/jasmine": "~2.8.6",
        "@types/jasminewd2": "~2.0.3",
        "@types/node": "~8.9.4",
        "codelyzer": "~4.2.1",
        "coveralls": "^3.0.1",
        "jasmine-core": "~2.99.1",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "~1.7.1",
        "karma-chrome-launcher": "~2.2.0",
        "karma-coverage-istanbul-reporter": "~1.4.2",
        "karma-jasmine": "~1.1.1",
        "karma-jasmine-html-reporter": "^0.2.2",
        "ng-packagr": "^3.0.0-rc.2",
        "protractor": "~5.3.0",
        "ts-node": "~5.0.1",
        "tsickle": ">=0.25.5",
        "tslib": "^1.7.1",
        "tslint": "~5.9.1",
        "typescript": "~2.7.2",
        "cpx": "1.5.0"
    }
}
